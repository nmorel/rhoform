  },
  "peerDependencies": {
    "react": ">=15"
  },
  "devDependencies": {
    "babel-cli": "^6.23.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-1": "^6.22.0",
    "react": "^15.4.2"
  },
  "dependencies": {
    "prop-types": "^15.5.8"
  }
}
