{
  "name": "rhoform",
  "version": "0.0.1",
  "description": "A React library to manage form",
  "main": "lib/index.js",
  "module": "es/index.js",
  "license": "MIT",
  "scripts": {
    "build": "rm -rf lib && rm -rf es && babel --no-babelrc src -d lib --presets=es2015,react,stage-1 && babel src -d es",
